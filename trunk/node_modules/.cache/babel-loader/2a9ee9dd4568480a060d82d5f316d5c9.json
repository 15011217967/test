CA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,SAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,GAAA,GAAA,IAAA,MAAA,EAAA;;AACA,cAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,YAAA,GAAA,CAAA,GAAA,GAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA;;AACA,YAAA,KAAA,CAAA,gBAAA,CAAA,IAAA,CAAA,GAAA;AACA,WAHA,MAGA;AACA,YAAA,GAAA,CAAA,GAAA,GAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA;;AACA,YAAA,KAAA,CAAA,iBAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,SAfA,CAgBA;AACA;AACA;AACA;;AACA,OApBA;AAqBA,KA5HA;AA6HA;AACA,IAAA,UA9HA,sBA8HA,QA9HA,EA8HA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,KAAA,GAAA,MAAA,CAAA,WAAA;AACA,cAAA,KAAA,GAAA,KAAA,CAAA,IAAA;;AACA,UAAA,KAAA,CAAA,qBAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,YAAA,IAAA,CAAA,cAAA,GAAA,KAAA;AACA,WAFA;;AAGA,UAAA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,GAAA,CAAA,OAAA,CAAA,kBAAA,EAAA,MAAA,CAAA,WAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,YAAA,UAAA,CAAA,YAAA;AACA,cAAA,MAAA,CAAA,UAAA;AACA,aAFA,EAEA,GAFA,CAAA;AAGA,WALA;AAOA,SAbA,MAaA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,iBAAA,KAAA;AACA;AACA,OAlBA;AAmBA,KAlJA;AAmJA;AACA,IAAA,UApJA,wBAoJA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AAtJA,GA/CA;AAuMA,EAAA,UAAA,EAAA;AACA,IAAA,WAAA,EAAA;AADA;AAvMA,CAAA","sourcesContent":["<template>\r\n  <div class=\"bg-fff item-margin\">\r\n    <el-form\r\n      class=\"item-form\"\r\n      :model=\"filtersData\"\r\n      :rules=\"rules\"\r\n      label-width=\"120px\"\r\n      size=\"small\"\r\n      ref=\"filtersForm\"\r\n      inline\r\n    >\r\n      <el-row>\r\n        <el-tabs v-model=\"activeName\" class=\"basic-wrap\">\r\n          <el-tab-pane label=\"基本信息\" name=\"basic\">\r\n            <el-col :span=\"24\">\r\n              <div class=\"flex-cen-start\">\r\n                <el-form-item prop=\"name\" label=\"设计商名称：\">\r\n                  <el-input\r\n                    class=\"input-w-2\"\r\n                    :disabled=\"editDisabled\"\r\n                    placeholder=\"请输入内容\"\r\n                    v-model=\"filtersData.name\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </div>\r\n              <div class=\"flex-cen-start\">\r\n                <el-form-item prop=\"contact\" label=\"联系人：\">\r\n                  <el-input\r\n                    class=\"input-w-2\"\r\n                    :disabled=\"showDisabled\"\r\n                    placeholder=\"请输入内容\"\r\n                    v-model=\"filtersData.contact\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </div>\r\n              <div class=\"flex-cen-start\">\r\n                <el-form-item prop=\"telephone\" label=\"联系电话：\">\r\n                  <el-input\r\n                    class=\"input-w-2\"\r\n                    :disabled=\"showDisabled\"\r\n                    placeholder=\"请输入内容\"\r\n                    v-model=\"filtersData.telephone\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </div>\r\n              <div class=\"flex-cen-start\">\r\n                <el-form-item prop=\"address\" label=\"设计商地址：\">\r\n                  <el-input\r\n                    class=\"input-w-2\"\r\n                    :disabled=\"showDisabled\"\r\n                    placeholder=\"请输入内容\"\r\n                    v-model=\"filtersData.address\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </div>\r\n              <div class=\"flex-cen-start\">\r\n                <el-form-item  label=\"设计商图标：\">\r\n                    <el-upload-img\r\n                        v-model=\"filtersData.logo\"\r\n                        img-size=\"1\"\r\n                        :EchoDisplayImages=\"productImages\"\r\n                        CatalogName=\"product\"\r\n                    ></el-upload-img>\r\n                </el-form-item>\r\n              </div>\r\n              <div class=\"flex-cen-start\">\r\n                <el-form-item prop=\"valid\"  label=\"合作状态：\">\r\n                  <el-radio v-model=\"filtersData.cooperation\" :disabled=\"showDisabled\" :label=\"true\">合作中</el-radio>\r\n                  <el-radio v-model=\"filtersData.cooperation\" :disabled=\"showDisabled\" :label=\"false\">未合作</el-radio>\r\n                </el-form-item>\r\n              </div>\r\n            </el-col>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"设计商资质\" name=\"brand_qcation\">\r\n            <el-col :span=\"24\">\r\n              <el-form-item prop label=\"资质图片：\">\r\n                <el-upload\r\n                  :action=\"action()\"\r\n                  :disabled=\"showDisabled\"\r\n                  list-type=\"picture-card\"\r\n                  accept=\"image/*\"\r\n                  :limit=\"10\"\r\n                  :headers=\"tokenHeaders\"\r\n                  :file-list=\"brandProductImgs\"\r\n                  :multiple=\"arrMultiple\"\r\n                  :on-preview=\"handlePictureCardPreview\"\r\n                  :on-remove=\"arrHandleRemove\"\r\n                  :on-success=\"arrHandleAvatarSuccess\"\r\n                  :data=\"uploadData\"\r\n                  :before-upload=\"beforeAvatarUpload\"\r\n                  :on-exceed=\"arrHandleExceed\"\r\n                  :on-error=\"arrImgUploadError\"\r\n                >\r\n                  <i class=\"el-icon-plus\" :disabled=\"showDisabled\"></i>\r\n                </el-upload>\r\n                <p class=\"img-tips\">上传商标信息、品牌信息等，单张图片大小不能超过2M</p>\r\n\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"合作协议\" name=\"cation_agrt\">\r\n            <el-col :span=\"24\">\r\n              <el-form-item prop label=\"合作协议：\">\r\n                <el-upload\r\n                  :action=\"action()\"\r\n                  :disabled=\"showDisabled\"\r\n                  list-type=\"picture-card\"\r\n                  accept=\"image/*\"\r\n                  :limit=\"10\"\r\n                  :headers=\"tokenHeaders\"\r\n                  :file-list=\"cationProductImgs\"\r\n                  :multiple=\"arrMultiple\"\r\n                  :on-preview=\"handlePictureCardPreview\"\r\n                  :on-remove=\"arrHandleRemove\"\r\n                  :on-success=\"arrTwoHandleAvatarSuccess\"\r\n                  :data=\"uploadData\"\r\n                  :before-upload=\"beforeAvatarUpload\"\r\n                  :on-exceed=\"arrHandleExceed\"\r\n                  :on-error=\"arrImgUploadError\"\r\n                >\r\n                  <i class=\"el-icon-plus\" :disabled=\"showDisabled\"></i>\r\n                </el-upload>\r\n                <p class=\"img-tips\">上传合作协议、授权信息等，单张图片大小不能超过2M</p>\r\n\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-tab-pane>\r\n        </el-tabs>\r\n      </el-row>\r\n    </el-form>\r\n    <div class=\"item-footer\">\r\n      <el-button\r\n        v-if=\"this._type == 'add'\"\r\n        size=\"small\"\r\n        @click=\"submitForm('filtersForm')\"\r\n        type=\"primary\"\r\n      >确定</el-button>\r\n      <el-button\r\n        v-else-if=\"this._type == 'edit'\"\r\n        size=\"small\"\r\n        @click=\"submitForm('filtersForm')\"\r\n        type=\"primary\"\r\n      >修改</el-button>\r\n      <el-button  size=\"small\" @click=\"routerback('filtersForm')\">取消</el-button>\r\n    </div>\r\n      <el-dialog :visible.sync=\"dialogVisible\">\r\n        <img width=\"100%\" :src=\"dialogImageUrl\" alt>\r\n      </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport ElUploadImg from \"@/components/uploadImg/el_upload_img\";\r\nimport { getToken } from \"@/utils/auth\";\r\nlet token = getToken();// 要保证取到\r\nexport default {\r\n  data() {\r\n    return {\r\n      filtersData: {\r\n        logo:\"\",\r\n        name: \"\",\r\n        address: \"\",\r\n        cooperation:true,\r\n        telephone:'',\r\n        valid: true,\r\n        designCompanyProfiles: []\r\n\r\n      },\r\n      activeName: \"basic\",\r\n      cateOptions: [],\r\n      rules: {\r\n        //规则验证\r\n        name: [\r\n          { required: true, message: \"请输入设计商名称\", trigger: \"blur\" }\r\n        ],\r\n        contact: [{ required: true, message: \"请输入联系人\", trigger: \"blur\" }],\r\n        telephone: [\r\n          { required: true, message: \"请输入联系电话\", trigger: \"blur\" },\r\n          {pattern: /^1[345789]\\d{9}$/, message: '请输入正确的手机号', trigger: 'blur'}\r\n        ]\r\n      },\r\n      _type: \"\",\r\n      _id: \"\",\r\n      productImages:[],\r\n      editDisabled: false,\r\n      showDisabled: false,\r\n      dialogImageUrl: \"\",\r\n      dialogVisible: false,\r\n      productImages: [],\r\n      isMultiple: false,\r\n      tokenHeaders: { accessToken: token },\r\n      uploadData: { catalog: \"product\" },\r\n      //多图上传\r\n      brandProductImgs: [], //多图展示列表 品牌资质\r\n      cationProductImgs: [], //多图展示列表 协议 合同\r\n      arrMultiple: true //是否支持多选文件\r\n    };\r\n  },\r\n  created() {\r\n    //获取上页面传参\r\n    this.getQueryparams();\r\n  },\r\n  methods: {\r\n      action() {\r\n          return this.api.FN_ALLPATH(this.api.content.picture_upload);\r\n      },\r\n    handlePictureCardPreview(file) {\r\n      //预览图片时调用\r\n      console.log(file);\r\n      this.dialogImageUrl = file.url;\r\n      this.dialogVisible = true;\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      //文件上传之前调用做一些拦截限制\r\n      console.log(file);\r\n      const isJPG = true;\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传图片大小不能超过 2MB!\");\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n    handleExceed(files, fileList) {\r\n      //图片上传超过数量限制\r\n      this.$message.error(\"logo只能上传一张!\");\r\n      console.log(file, fileList);\r\n    },\r\n    imgUploadError(err, file, fileList) {\r\n      //图片上传失败调用\r\n      console.log(err);\r\n      this.$message.error(\"上传图片失败!\");\r\n    },\r\n    //多图删除\r\n    arrHandleRemove(file, fileList) {\r\n      console.log(file);\r\n      let fileImgUrl = null;\r\n      if (file.response) {\r\n        fileImgUrl = file.response.data.imageUrl;\r\n      } else {\r\n        fileImgUrl = file.url;\r\n      }\r\n      let imgArr = this.filtersData.designCompanyProfiles;\r\n      let index = imgArr.findIndex(item => item.path == fileImgUrl);\r\n      if(imgArr[index].id != undefined){\r\n          this.request(this.api.product.enterpriseArchivalInformation_logicDelete,{id:imgArr[index].id});\r\n      }\r\n      imgArr.splice(index, 1);\r\n    },\r\n    ////图片上传成功 资质\r\n    arrHandleAvatarSuccess(res, file) {\r\n      //图片上传成功\r\n      //console.log(res.data.imageUrl);\r\n      let obj = new Object();\r\n      obj.enterpriseType = 3; //1品牌 2 供应商 3设计商 4制造商\r\n      obj.type = 1; //资质 1  协议2\r\n      obj.path = res.data.imageUrl;\r\n      obj.enterpriseName = \"\";\r\n      this.filtersData.designCompanyProfiles.push(obj);\r\n      // console.log( this.filtersData.designCompanyProfiles);\r\n    }, //协议\r\n    arrTwoHandleAvatarSuccess(res, file) {\r\n      // console.log(res.data.imageUrl);\r\n      let obj = new Object();\r\n      obj.enterpriseType = 3; //1品牌 2 供应商 3设计商 4制造商\r\n      obj.type = 2; //资质 1  协议2\r\n      obj.path = res.data.imageUrl;\r\n      obj.enterpriseName = \"\";\r\n      this.filtersData.designCompanyProfiles.push(obj);\r\n      //console.log( this.filtersData.designCompanyProfiles);\r\n    },\r\n    arrHandleExceed(files, fileList) {\r\n      //图片上传超过数量限制\r\n      this.$message.error(\"只能上传10张!\");\r\n      console.log(file, fileList);\r\n    },\r\n    arrImgUploadError(err, file, fileList) {\r\n      //图片上传失败调用\r\n      console.log(err);\r\n      this.$message.error(\"上传图片失败!\");\r\n    },\r\n    //获取上页面参数\r\n    getQueryparams() {\r\n      //console.log(this.$route.query.id);\r\n      this._type = this.$route.query.type;\r\n      this._id = this.$route.query.id;\r\n      let itemStatus = this._type;\r\n      switch (itemStatus.toString()) {\r\n        case \"add\": //新增\r\n          this.editDisabled = false;\r\n          this.showDisabled = false;\r\n          break;\r\n        case \"edit\": //修改\r\n          this.editDisabled = true;\r\n          this.showDisabled = false;\r\n          this.getData();\r\n          break;\r\n        case \"show\": //查看\r\n          this.editDisabled = true;\r\n          this.showDisabled = true;\r\n          this.getData();\r\n          break;\r\n      }\r\n    },\r\n    //获取数据\r\n    getData() {\r\n       this.request(this.api.product.designCompany_get, { id: this._id }).then(data => {\r\n        let _data = data;\r\n        let _Profiles = _data.designCompanyProfiles;\r\n        this.filtersData = _data; //designCompanyProfiles\r\n        this.productImages = [{ url: _data.logo }]; //logo\r\n        //type 资质 1  协议2\r\n        for (let i = 0; i < _Profiles.length; i++) {\r\n          let obj = new Object();\r\n          if (_Profiles[i].type == 1) {\r\n            obj.url = _Profiles[i].path;\r\n            this.brandProductImgs.push(obj);\r\n          } else {\r\n            obj.url = _Profiles[i].path;\r\n            this.cationProductImgs.push(obj);\r\n          }\r\n        }\r\n        //资质\r\n        // brandProductImgs:[],//多图展示列表 品牌资质\r\n        //合同协议\r\n        //cationProductImgs:[],//多图展示列表 协议 合同\r\n      });\r\n    },\r\n    //新增保存\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          let _data = this.filtersData;\r\n          let _name = _data.name;\r\n          _data.designCompanyProfiles.forEach((item, index) => {\r\n            item.enterpriseName = _name;\r\n          });\r\n            this.request(this.api.product.designCompany_save, this.filtersData).then(data => {\r\n                this.$message.success('保存成功');\r\n                setTimeout(() => {\r\n                    this.routerback();\r\n                }, 500);\r\n            }\r\n          );\r\n        } else {\r\n          console.log(\"error submit!!\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //后退\r\n    routerback() {\r\n      this.$router.go(-1);\r\n    }\r\n  },\r\n   components: {\r\n    ElUploadImg\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/product/supplier"}]}