    for (var i = 0; i < _Profiles.length; i++) {\n          var obj = new Object();\n\n          if (_Profiles[i].type == 1) {\n            obj.url = _Profiles[i].path;\n\n            _this.brandProductImgs.push(obj);\n          } else {\n            obj.url = _Profiles[i].path;\n\n            _this.cationProductImgs.push(obj);\n          }\n        } //资质\n        // brandProductImgs:[],//多图展示列表 品牌资质\n        //合同协议\n        //cationProductImgs:[],//多图展示列表 协议 合同\n\n      });\n    },\n    //新增\n    submitForm: function submitForm(formName) {\n      var _this2 = this;\n\n      this.$refs[formName].validate(function (valid) {\n        if (valid) {\n          var _data = _this2.filtersData;\n          var _name = _data.name;\n\n          _data.productBrandProfiles.forEach(function (item, index) {\n            item.enterpriseName = _name;\n          });\n\n          _this2.request(_this2.api.product.productBrand_save, _this2.filtersData).then(function (data) {\n            _this2.$router.go(-1);\n          });\n        } else {\n          console.log(\"error submit!!\");\n          return false;\n        }\n      });\n    },\n    //后退\n    routerback: function routerback() {\n      this.$router.go(-1);\n    }\n  }\n};",{"version":3,"sources":["brand_details.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmKA;;;AAGA,SAAA,QAAA,QAAA,cAAA;AAEA,IAAA,KAAA,GAAA,QAAA,EAAA,C,CAAA;;AACA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,WAAA,EAAA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA,EAHA;AAIA,QAAA,WAAA,EAAA,IAJA;AAKA,QAAA,KAAA,EAAA,IALA;AAMA,QAAA,oBAAA,EAAA,EANA,CAMA;;AANA,OADA;AASA;AACA,MAAA,UAAA,EAAA,OAVA;AAWA,MAAA,KAAA,EAAA;AACA;AACA,QAAA,IAAA,EAAA,CAAA;AAAA,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,EAAA,SAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,CAFA;AAGA,QAAA,IAAA,EAAA,CAAA;AAAA,UAAA,QAAA,EAAA,IAAA;AAAA,UAAA,OAAA,E