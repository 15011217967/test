: 10\n      }\n    };\n  },\n  filters: {\n    //过滤当前状态\n    filterBusinessStatus: function filterBusinessStatus(val) {\n      switch (val) {\n        case \"06\":\n          return \"未完成\";\n          break;\n\n        case \"08\":\n          return \"已完成\";\n          break;\n\n        default:\n          return '--';\n          break;\n      }\n    }\n  },\n  mounted: function mounted() {\n    this.shopData.orderSn = this.$route.query.orderSn;\n    this.getPurchasDetailListMsg();\n  },\n  methods: {\n    //顶部小箭头，点击返回ditch路由页面\n    backToPreRoute: function backToPreRoute() {\n      this.$router.push({\n        path: \"/OS/channelSales/channelSales\"\n      }); // this.$route.go(-1)\n    },\n    //得到表格数据\n    getPurchasDetailListMsg: function getPurchasDetailListMsg() {\n      var _this = this;\n\n      this.request(this.api.order.channelPurchaseOrder_getByOrderSn, this.shopData).then(function (data) {\n        console.log(data);\n        console.log(\"145=============\");\n        _this.person = data;\n\n        if (data.channelOrderGoodsVOList.data) {\n          data.channelOrderGoodsVOList.data.forEach(function (item) {\n            if (data.tax && data.tax != '0') {\n              item.tax = data.tax;\n              item.amount = item.num * item.actualAmount;\n            } else {\n              item.amount = item.num * item.purchasePrice;\n            }\n          });\n        }\n\n        _this.orderShopData = data.channelOrderGoodsVOList.data;\n        _this.totalPage = data.channelOrderGoodsVOList.totalCount;\n      });\n    },\n    //点击查看更多商品\n    showOrderShopView: function showOrderShopView() {\n      console.log(\"showOrderShopView\");\n    },\n    //每页条数改变时\n    handleSizeChange: function handleSizeChange(page) {\n      console.log(page);\n    },\n    // 当前页改变时\n    handleCurrentChange: function handleCurrentChange(page) {\n      console.log(page);\n    }\n  }\n};",{"version":3,"sources":["purchaseOrder_check.vue"],"names":[],"mappings":";;;;;;;;;;;;;;