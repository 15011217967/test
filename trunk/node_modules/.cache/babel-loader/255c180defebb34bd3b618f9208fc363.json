g\r\n      title=\"账号存在多组织，请选择：\"\r\n      :visible.sync=\"dialogVisible\"\r\n      class=\"alarmPopup\"\r\n      width=\"400px\"\r\n      :show-close=\"false\"\r\n      :before-close=\"closeDialog\"\r\n    >\r\n        <ul class=\"wrap\">\r\n          <li  v-for=\"(item,index) in tenantList\"\r\n              :key=\"index\"\r\n              @click=\"itemClick(item,index)\"\r\n              :class=\"{'active':item.isSelected}\">{{item.name}}</li>\r\n        </ul>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n       tenantId:'',\r\n       tenantList:[],\r\n       type:'',\r\n    };\r\n  },\r\n  created() {\r\n  },\r\n  methods: {\r\n     propInit(arr,type) {\r\n      for (let k = 0; k < arr.length; k++) {\r\n        arr[k].isSelected = false;\r\n      }\r\n      this.tenantList = arr;\r\n      this.type=type;\r\n      this.dialogVisible = true;\r\n    },\r\n    itemClick(item){\r\n       item.isSelected = !item.isSelected;\r\n       this.tenantId = item.id ? item.id : \"\";\r\n       if(!!item.isSelected){\r\n         this.$emit('change',this.tenantId,this.type);\r\n         this.dialogVisible=false;\r\n       }\r\n       this.noClick(item);\r\n    },\r\n    // 置灰其他未选\r\n    noClick(item) {\r\n      for (let i = 0; i < this.tenantList.length; i++) {\r\n        if (item.id != this.tenantList[i].id) {\r\n          this.tenantList[i].isSelected = false;\r\n        }\r\n      }\r\n    },\r\n    closeDialog(done) {\r\n      if(!this.tenantId){\r\n        this.$message.error('请选择归属货主！');\r\n        return false;\r\n      }\r\n      done();\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.wrap{\r\n  >li{\r\n    width: 100%;\r\n    height: 60px;\r\n    text-align: center;\r\n    line-height: 60px;\r\n    font-size: 14px;\r\n    cursor: pointer;\r\n    border: 1px solid transparent;\r\n\r\n  }\r\n  >li.active{\r\n    background-color: #f7fcff;\r\n    colo