;AACA,kBAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,qBAAA,CAAA,QAAA,CAAA,GAAA,EAAA,IAAA;AACA,eAFA,MAEA,IAAA,GAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,CAAA,CAAA,CAAA,EAAA,CAFA,CAGA;;AACA,oBAAA,IAAA,IAAA,KAAA,EAAA;AACA,kBAAA,MAAA,CAAA,OAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,gBAAA;AACA;AACA;AACA,aAhBA,EAiBA,KAjBA,CAiBA,UAAA,GAAA,EAAA,CAAA,CAjBA;AAkBA,WAnBA,MAmBA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA;;AACA,mBAAA,KAAA;AACA;AACA,SAhCA,MAgCA;AACA,UAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,iBAAA,KAAA;AACA;AACA,OArCA;AAsCA,KAzJA;AA0JA,IAAA,YA1JA,wBA0JA,EA1JA,EA0JA;AAAA;;AACA,UAAA,YAAA,CAAA,EAAA,CAAA,MAAA,CAAA,KAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA,KAAA,GAAA,CAAA,IAAA,CAAA,gCAAA,EAAA;AACA,UAAA,QAAA,EAAA,EAAA,CAAA,MAAA,CAAA;AADA,SAAA,EAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,qBAAA,CAAA,QAAA,CAAA,GAAA;AACA,WAFA,MAEA,IAAA,GAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,CAAA,CAAA,CAAA,EAAA;AACA;AACA,SATA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA,CAAA,CAVA;AAWA;AACA,KAxKA;AAyKA;AACA,IAAA,SA1KA,qBA0KA,IA1KA,EA0KA,IA1KA,EA0KA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,IAAA;;AACA,UAAA,IAAA,IAAA,KAAA,EAAA;AACA,aAAA,OAAA;AACA,OAFA,MAEA;AACA,aAAA,gBAAA;AACA;AACA;AAlLA,IAxCA;AA4NA,EAAA,KAAA,EAAA;AACA,yBAAA,0BAAA,GAAA,EAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA;AAHA;AA5NA,CAAA","sourcesContent":["<template>\n  <div class=\"login-wrap\">\n    <!-- 浏览器记住密码解决办法 -->\n    <!-- <input type=\"password\" name=\"password\" style=\"position: absolute;left: -10000px;\"> -->\n    <!--  -->\n    <el-form\n      ref=\"ruleForm\"\n      label-position=\"left\"\n      :model=\"ruleForm\"\n      :rules=\"rules\"\n      label-width=\"0px\"\n      class=\"demo-ruleForm login-container\"\n    >\n      <el-tabs v-model=\"activeName\" class=\"loginClick\" @tab-click=\"handleClick\">\n        <el-tab-pane label=\"密码登录\" name=\"first\">\n          <div v-if=\"isChildUpdate1\">\n            <el-form-item prop=\"phoneNum\">\n              <el-input\n                v-model=\"ruleForm.phoneNum\"\n                type=\"text\"\n                auto-complete=\"off\"\n                placeholder=\"请输入手机号\"\n                @keyup.enter.native=\"handleForm('pwd')\"\n                maxlength=\"11\"\n              />\n            </el-form-item>\n            <el-form-item prop=\"pwd\" class=\"pwdLogin\">\n              <el-input\n                v-model=\"ruleForm.pwd\"\n                type=\"password\"\n                auto-complete=\"off\"\n                placeholder=\"请输入密码\"\n                @keyup.enter.native=\"handleForm('pwd')\"\n              />\n            </el-form-item>\n            <div class=\"forgetDiv\">\n              <el-button size=\"mini\" class=\"forgetPassword\" @click=\"forgetPassword\">忘记密码？</el-button>\n            </div>\n            <el-form-item style=\"width:100%;\">\n              <el-button\n                type=\"primary\"\n                style=\"width:100%;\"\n                :loading=\"logining\"\n                @click=\"handleForm('pwd')\"\n              >登录</el-button>\n            </el-form-item>\n          </div>\n        </el-tab-pane>\n        <el-tab-pane label=\"验证码登录\" name=\"second\">\n          <div v-if=\"isChildUpdate2\">\n            <el-form-item prop=\"phoneNum\">\n              <el-input\n                v-model=\"ruleForm.phoneNum\"\n                type=\"text\"\n                auto-complete=\"off\"\n                placeholder=\"请输入手机号\"\n                @keyup.enter.native=\"handleForm('code')\"\n                maxlength=\"11\"\n              />\n            </el-form-item>\n            <el-form-item prop=\"code\">\n              <el-input\n                v-model=\"ruleForm.code\"\n                auto-complete=\"off\"\n                placeholder=\"请输入验证码\"\n                class=\"verification\"\n                @keyup.enter.native=\"handleForm('code')\"\n              />\n              <div class=\"codeClick\" @click=\"codeClickSubmit\">\n                <!-- 获取验证码 -->\n                {{ buttonText }}\n              </div>\n            </el-form-item>\n            <el-form-item style=\"width:100%;\">\n              <el-button\n                type=\"primary\"\n                style=\"width:100%;\"\n                :loading=\"logining\"\n                @click=\"handleForm('code')\"\n              >登录</el-button>\n            </el-form-item>\n          </div>\n        </el-tab-pane>\n      </el-tabs>\n\n      <div>\n        <div class=\"hint\">还没有账号？</div>\n        <el-form-item style=\"width:100%;\">\n          <el-button plain style=\"width:100%;\" @click=\"registerForm\">立即注册</el-button>\n        </el-form-item>\n      </div>\n    </el-form>\n    <!-- 选择组织弹出框 -->\n    <SelectOrganization ref=\"SelectOrganizationRef\" @change=\"getTenant\"></SelectOrganization>\n  </div>\n</template>\n<script type=\"text/ecmascript-6\">\nimport { setToken } from \"../../utils/auth\";\nimport * as userTypes from \"@/types/userConfig\";\nimport * as codeTypes from \"@/types/codeset\";\nimport { mapMutations, mapActions } from \"vuex\";\nimport { isvalidPhone } from \"@/utils/validate\";\nimport SelectOrganization from \"@/components/Modal/SelectOrganization.vue\";\nexport default {\n  components: {\n    SelectOrganization\n  },\n  data() {\n    return {\n      // 定义loading默认为false\n      logining: false,\n      // 记住密码\n      // 18612443290/123123\n      ruleForm: {\n        phoneNum: \"\",\n        pwd: \"\",\n        code: \"\"\n      },\n      tenantId: null,\n      tenantStatus: false,\n      rules: {\n        phoneNum: [\n          { required: true, message: \"请输入手机号\", trigger: \"blur\" }\n        ],\n        pwd: [{ required: true, message: \"请输入密码\", trigger: \"blur\" }],\n        code: [{ required: true, message: \"请输入验证码\", trigger: \"blur\" }]\n      },\n      activeName: \"first\",\n      isChildUpdate1: true,\n      isChildUpdate2: false,\n      buttonText: \"发送验证码\",\n      isDisabled: false, // 是否禁止点击发送验证码按钮\n      flag: true\n    };\n  },\n  // 创建完毕状态(里面是操作)\n  created() {\n    // 获取存在本地的用户名密码\n    // 清空缓存\n    localStorage.clear();\n    sessionStorage.clear();\n  },\n  // 里面的函数只有调用才会执行\n  methods: {\n    ...mapMutations({\n      FN_SET_TOKEN: userTypes.FN_SET_TOKEN\n    }),\n    ...mapActions({\n      FN_CODESET_LOAD: codeTypes.FN_LOAD_DATA\n    }),\n    handleClick(tab, event) {\n      if (tab.name == \"first\") {\n        this.isChildUpdate1 = true;\n        this.isChildUpdate2 = false;\n        this.ruleForm.phoneNum == \"\";\n        this.ruleForm.pwd == \"\";\n      } else if (tab.name == \"second\") {\n        this.isChildUpdate1 = false;\n        this.isChildUpdate2 = true;\n        this.ruleForm.phoneNum == \"\";\n        this.ruleForm.code == \"\";\n      }\n    },\n    // 密码登录\n    pwdForm() {\n      this.logining = true;\n      this.request(this.api.user.login_loginByPwd, {\n        phoneNum: this.ruleForm.phoneNum,\n        pwd: this.ruleForm.pwd,\n        tenantId: this.tenantId\n      })\n        .then(res => {\n          this.FN_SET_TOKEN(res.token);\n          this.FN_CODESET_LOAD();\n          // 存储登陆人名称\n          localStorage.setItem(\"userName\", res.userInfo.userName);\n          localStorage.setItem(\"tenantId\", res.userInfo.tenantId);\n          this.$router.push({ path: \"/home\" });\n        })\n        .catch(res => {\n          this.logining = false;\n        });\n    },\n    // 获取验证码\n    codeClickSubmit() {\n      if (this.ruleForm.phoneNum != \"\") {\n        this.request(this.api.user.login_sendCode, {\n          phoneNum: this.ruleForm.phoneNum,\n          type: 2\n        })\n          .then(res => {\n            // console.log(res)\n            const { phoneNum } = this.ruleForm;\n            if (this.checkMobile(phoneNum)) {\n              let time = 60;\n              this.buttonText = \"已发送\";\n              this.isDisabled = true;\n              if (this.flag) {\n                this.flag = false;\n                const timer = setInterval(() => {\n                  time--;\n                  this.buttonText = `${time} 秒`;\n                  if (time === 0) {\n                    clearInterval(timer);\n                    this.buttonText = \"重新获取\";\n                    this.isDisabled = false;\n                    this.flag = true;\n                  }\n                }, 1000);\n              }\n            }\n          })\n          .catch(res => {});\n      } else {\n        this.$message.error(\"请输入手机号\");\n        return false;\n      }\n    },\n    // 验证码登录\n    PhoneAndCodeForm() {\n      this.logining = true;\n      this.request(this.api.user.login_loginByPhoneAndCode, {\n        phoneNum: this.ruleForm.phoneNum,\n        code: this.ruleForm.code,\n        tenantId: this.tenantId\n      })\n        .then(res => {\n          // 存储登陆人名称\n          localStorage.setItem(\"userName\", res.userInfo.userName);\n          localStorage.setItem(\"tenantId\", res.userInfo.tenantId);\n          //存储登录Token\n          this.FN_SET_TOKEN(res.token);\n          //得到codeData数据\n          this.FN_CODESET_LOAD();\n          this.$router.push({ path: \"/home\" });\n        })\n        .catch(res => {\n          this.logining = false;\n        });\n    },\n    // 验证手机号\n    checkMobile(str) {\n      const re = /^1\\d{10}$/;\n      if (re.test(str)) {\n        return true;\n      }\n      return false;\n    },\n    // 立即注册\n    registerForm() {\n      this.$router.push({ path: \"/login/register\" });\n    },\n    // 忘记密码\n    forgetPassword() {\n      this.$router.push({ path: \"/login/find\" });\n    },\n    // 处理提交\n    handleForm(type) {\n      this.$refs.ruleForm.validate(valid => {\n        if (valid) {\n          if (!!this.tenantId && !!this.tenantStatus) {\n            if (type == \"pwd\") {\n              this.pwdForm();\n            } else {\n              this.PhoneAndCodeForm();\n            }\n            return false;\n          }\n          if (isvalidPhone(this.ruleForm.phoneNum)) {\n            this.request(this.api.user.userCompany_loginCheckTenantData, {\n              phoneNum: this.ruleForm.phoneNum\n            })\n              .then(res => {\n                if (res.length > 1) {\n                  this.$refs.SelectOrganizationRef.propInit(res, type);\n                } else if (res.length == 1) {\n                  this.tenantStatus = true;\n                  this.tenantId = res[0].id;\n                  // 判断是code 提交还是 pwd 提交\n                  if (type == \"pwd\") {\n                    this.pwdForm();\n                  } else {\n                    this.PhoneAndCodeForm();\n                  }\n                }\n              })\n              .catch(err => {});\n          } else {\n            this.$message.error(\"请输入正确的手机号！\");\n            return false;\n          }\n        } else {\n          this.logining = false;\n          return false;\n        }\n      });\n    },\n    phoneNumBlur(el) {\n      if (isvalidPhone(el.target.value)) {\n        this.request(this.api.user.userCompany_loginCheckTenantData, {\n          phoneNum: el.target.value\n        })\n          .then(res => {\n            if (res.length > 1) {\n              this.$refs.SelectOrganizationRef.propInit(res);\n            } else if (res.length == 1) {\n              this.tenantId = res[0].id;\n            }\n          })\n          .catch(err => {});\n      }\n    },\n    // 返回货主\n    getTenant(item, type) {\n      this.tenantId = item;\n      this.tenantStatus = true;\n      if (type == \"pwd\") {\n        this.pwdForm();\n      } else {\n        this.PhoneAndCodeForm();\n      }\n    }\n  },\n  watch: {\n    \"ruleForm.phoneNum\": function(val) {\n      this.tenantStatus = false;\n    }\n  }\n};\n</script>\n<style scoped>\n.loginClick /deep/ .is-top .el-tabs__item {\n  font-weight: 800;\n  text-align: center;\n  font-size: 20px;\n}\n.loginClick /deep/ .is-top .el-tabs__active-bar {\n  background-color: #fff;\n}\n.loginClick /deep/ .is-top .el-tabs__nav-wrap::after {\n  background-color: #fff;\n}\n.loginClick /deep/ .is-top {\n  margin: 0 7px;\n}\n.verification {\n  width: 60%;\n  float: left;\n}\n.codeClick {\n  width: 40%;\n  float: left;\n  text-align: center;\n  cursor: pointer;\n}\n.hint {\n  color: #999;\n  font-size: 14px;\n  font-weight: 500;\n  margin-bottom: 22px;\n}\n.hint:before,\n.hint:after {\n  content: \"\";\n  width: 83px;\n  border-top: 1px #999 solid;\n  display: inline-block;\n  vertical-align: middle;\n}\n.hint:before {\n  margin-right: 10px;\n}\n/* .hint:after{\n  margin-left: 10px;\n} */\n.forgetDiv {\n  text-align: right;\n}\n.forgetPassword {\n  border: 0;\n  background: none;\n  padding: 0;\n}\n.pwdLogin {\n  margin-bottom: -5px;\n}\n\n.login-wrap {\n  box-sizing: border-box;\n  width: 100%;\n  height: 100%;\n  padding-top: 10%;\n  background-image: url(data:image/svg+xml;base64,);\n  /* background-color: #112346; */\n  background-repeat: no-repeat;\n  background-position: center right;\n  background-size: 100%;\n}\n.login-container {\n  border-radius: 10px;\n  margin: 0px auto;\n  width: 350px;\n  padding: 30px 35px 15px 35px;\n  background: #fff;\n  border: 1px solid #eaeaea;\n  text-align: left;\n  box-shadow: 0 0 20px 2px rgba(0, 0, 0, 0.1);\n}\n.title {\n  margin: 0px auto 40px auto;\n  text-align: center;\n  color: #505458;\n}\n.remember {\n  margin: 0px 0px 35px 0px;\n}\n.code-box {\n  text-align: right;\n}\n.codeimg {\n  height: 40px;\n}\n</style>\n"],"sourceRoot":"src/views/login"}]}