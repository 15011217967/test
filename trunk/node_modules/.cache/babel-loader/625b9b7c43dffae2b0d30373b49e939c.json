el-option>\r\n            <el-option label=\"02\" value=\"06\"></el-option>\r\n            <el-option label=\"03\" value=\"07\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"货位：\" prop=\"area\" label-width=\"90px\">\r\n          <el-input v-model=\"signData.allocator\" placeholder=\"请输入货位\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item style=\"margin-left:20px;\">\r\n          <el-button type=\"primary\" plain @click=\"onSearchContent\" size=\"small\">搜索</el-button>\r\n          <el-button @click=\"resetForm('ruleForm')\" size=\"small\">重置</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n\r\n    <!-- table -->\r\n    <div class=\"table\">\r\n      <el-button\r\n        type=\"primary\"\r\n        @click=\"addBtnClickEvent\"\r\n        size=\"small\"\r\n      >新增货位</el-button>\r\n      <el-button\r\n        @click=\"leadBtnClick\"\r\n        style=\"margin-bottom:20px;\"\r\n        size=\"small\"\r\n        type=\"primary\"\r\n        plain\r\n      >导入货位</el-button>\r\n      <el-button\r\n        plain\r\n        @click=\"exportExcel\"\r\n        size=\"small\"\r\n      >导出</el-button>\r\n      <!-- table订单 -->\r\n      <template>\r\n        <el-table\r\n          ref=\"multipleTable\"\r\n          :data=\"areaTableData\"\r\n          tooltip-effect=\"dark\"\r\n          style=\"width: 100%\"\r\n          @selection-change=\"handleSelectionChange\"\r\n          header-row-class-name=\"item-table-header\"\r\n        >\r\n          <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n          <el-table-column label=\"仓库名称\" prop=\"orderSn\"></el-table-column>\r\n          <el-table-column prop=\"storeName\" label=\"货区名称\">\r\n            <template slot-scope=\"scope\">\r\n              <span>{{scope.row.storeName ? scope.row.storeName:'--'}}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"receiverNa