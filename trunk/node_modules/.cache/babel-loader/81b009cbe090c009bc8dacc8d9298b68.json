=\"password\" name=\"password\" style=\"position: absolute;left: -10000px;\"> -->\n    <!--  -->\n    <el-form\n      ref=\"ruleForm\"\n      label-position=\"left\"\n      :model=\"ruleForm\"\n      :rules=\"rules\"\n      label-width=\"0px\"\n      class=\"demo-ruleForm login-container\"\n    >\n      <el-tabs v-model=\"activeName\" class=\"loginClick\" @tab-click=\"handleClick\">\n        <el-tab-pane label=\"密码登录\" name=\"first\">\n          <div v-if=\"isChildUpdate1\">\n            <el-form-item prop=\"phoneNum\">\n              <el-input\n                v-model=\"ruleForm.phoneNum\"\n                type=\"text\"\n                auto-complete=\"off\"\n                placeholder=\"请输入账号\"\n                @keyup.enter.native=\"pwdForm('ruleForm')\"\n                maxlength=\"11\"\n              />\n            </el-form-item>\n            <el-form-item prop=\"pwd\" class=\"pwdLogin\">\n              <el-input\n                v-model=\"ruleForm.pwd\"\n                type=\"password\"\n                auto-complete=\"off\"\n                placeholder=\"请输入密码\"\n                @keyup.enter.native=\"pwdForm('ruleForm')\"\n              />\n            </el-form-item>\n            <div class=\"forgetDiv\">\n              <el-button\n                size=\"mini\"\n                class=\"forgetPassword\"\n                @click=\"forgetPassword\"\n              >\n                忘记密码？\n              </el-button>\n            </div>\n            <el-form-item style=\"width:100%;\">\n              <el-button\n                type=\"primary\"\n                style=\"width:100%;\"\n                :loading=\"logining\"\n                @click=\"pwdForm('ruleForm')\"\n              >\n                登录\n              </el-button>\n            </el-form-item>\n          </div>\n        </el-tab-pane>\n        <el-tab-pane label=\"验证码登录\" name=\"second\">\n          <div v-if=\"isChildUpdate2\">\n            <el-form-item prop=\"phoneNum\">\n              <el-input\n                v-model=\"ruleForm.phoneNum\"\n                type=\"text\"\n                auto-complete=\"off\"\n                placeholder=\"请输入账号\"\n                @keyup.enter.native=\"PhoneAndCodeForm('ruleForm')\"\n                maxlength=\"11\"\n              />\n            </el-form-item>\n            <el-form-item prop=\"code\">\n              <el-input\n                v-model=\"ruleForm.code\"\n                auto-complete=\"off\"\n                placeholder=\"请输入验证码\"\n                class=\"verification\"\n                @keyup.enter.native=\"PhoneAndCodeForm('ruleForm')\"\n              />\n              <div class=\"codeClick\" @click=\"codeClickSubmit\">\n                获取验证码\n              </div>\n            </el-form-item>\n            <el-form-item style=\"width:100%;\">\n              <el-button\n                type=\"primary\"\n                style=\"width:100%;\"\n                :loading=\"logining\"\n                @click=\"PhoneAndCodeForm('ruleForm')\"\n              >\n                登录\n              </el-button>\n            </el-form-item>\n          </div>\n        </el-tab-pane>\n      </el-tabs>\n\n      <div>\n        <div class=\"hint\">\n          还没有账号？\n        </div>\n        <el-form-item style=\"width:100%;\">\n          <el-button\n            plain\n            style=\"width:100%;\"\n            @click=\"registerForm\"\n          >\n            立即注册\n          </el-button>\n        </el-form-item>\n      </div>\n    </el-form>\n  </div>\n</template>\n<script type=\"text/ecmascript-6\">\nimport { setToken } from \"../../utils/auth\";\nimport * as userTypes from \"@/types/userConfig\";\nimport * as codeTypes from \"@/types/codeset\";\nimport { mapMutations, mapActions } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      // 定义loading默认为false\n      logining: false,\n      // 记住密码\n      // 18612443290/123123\n      ruleForm: {\n        phoneNum: '',\n        pwd: '',\n        code: ''\n      },\n      // rules前端验证\n      rules: {\n        phoneNum: [{ required: true, message: '请输入账号', trigger: 'blur' }],\n        pwd: [{ required: true, message: '请输入密码', trigger: 'blur' }],\n        code: [{ required: true, message: '请输入验证码', trigger: 'blur' }]\n      },\n      activeName: 'first',\n      isChildUpdate1: true,\n      isChildUpdate2: false\n    }\n  },\n  // 创建完毕状态(里面是操作)\n  created() {\n    // 获取存在本地的用户名密码\n    // 清空缓存\n    localStorage.clear()\n    sessionStorage.clear()\n    // this.getuserpwd();\n  },\n  // 里面的函数只有调用才会执行\n  methods: {\n    ...mapMutations({\n      FN_SET_TOKEN: userTypes.FN_SET_TOKEN\n    }),\n    ...mapActions({\n      FN_CODESET_LOAD: codeTypes.FN_LOAD_DATA\n    }),\n    handleClick(tab, event) {\n      console.log(tab, event);\n      if (tab.name == 'first') {\n        this.isChildUpdate1 = true;\n        this.isChildUpdate2 = false;\n        // console.log(11111111111)\n        this.ruleForm.phoneNum == ''\n        this.ruleForm.pwd == ''\n      } else if (tab.name == 'second') {\n        this.isChildUpdate1 = false;\n        this.isChildUpdate2 = true;\n        // console.log(22222222)\n        this.ruleForm.phoneNum == ''\n        this.ruleForm.code == ''\n      }\n\n    },\n\n    // 密码登录\n    pwdForm(formName) {\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n          this.logining = true\n          this.request(this.api.user.login_loginByPwd, { phoneNum: this.ruleForm.phoneNum, pwd: this.ruleForm.pwd }).then((res) => {\n            this.FN_SET_TOKEN(res.token);\n            this.FN_CODESET_LOAD();\n            // 存储登陆人名称\n            localStorage.setItem('userName', res.userInfo.userName)\n            localStorage.setItem('tenantId', res.userInfo.tenantId)\n            this.$router.push({ path: '/home' })\n          }).catch((res) => {\n            this.logining = false\n          })\n        } else {\n          this.logining = false\n          return false\n        }\n      })\n    },\n    // 获取验证码\n    codeClickSubmit() {\n      if (this.ruleForm.phoneNum != '') {\n        this.request(this.api.user.login_sendCode, { phoneNum: this.ruleForm.phoneNum }).then((res) => {\n          // console.log(res)\n          const { phoneNum } = this.ruleForm\n          // let phoneNum = this.ruleForm.phoneNum\n          if (this.checkMobile(phoneNum)) {\n            console.log(phoneNum)\n            let time = 60\n            this.buttonText = '已发送'\n            this.isDisabled = true\n            if (this.flag) {\n              this.flag = false;\n              const timer = setInterval(() => {\n                time--;\n                this.buttonText = `${time} 秒`\n                if (time === 0) {\n                  clearInterval(timer);\n                  this.buttonText = '重新获取'\n                  this.isDisabled = false\n                  this.flag = true;\n                }\n              }, 1000)\n            }\n          }\n        }).catch((res) => {\n\n        })\n      } else {\n        this.$message.error('请输入账号')\n        return false;\n      }\n\n    },\n    // 验证码登录\n    PhoneAndCodeForm(formName) {\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n          this.logining = true\n          this.request(this.api.user.login_loginByPhoneAndCode, { phoneNum: this.ruleForm.phoneNum, code: this.ruleForm.code }).then((res) => {\n            console.log(res)\n            // 存储登陆人名称\n            localStorage.setItem('userName', res.userInfo.userName)\n            localStorage.setItem('tenantId', res.userInfo.tenantId)\n            //存储登录Token\n            this.FN_SET_TOKEN(res.token);\n            //得到codeData数据\n            this.FN_CODESET_LOAD();\n            this.$router.push({ path: '/home' })\n          }).catch((res) => {\n            this.logining = false\n          })\n        } else {\n          this.logining = false\n          return false\n        }\n      })\n    },\n    // 验证手机号\n    checkMobile(str) {\n      const re = /^1\\d{10}$/\n      if (re.test(str)) {\n        return true;\n      }\n      return false;\n    },\n    // 立即注册\n    registerForm() {\n      console.log(1111111111)\n      this.$router.push({ path: '/login/register' })\n    },\n    // 忘记密码\n    forgetPassword() {\n      this.$router.push({ path: '/login/find' })\n    },\n  }\n}\n</script>\n\n<style scoped>\n.loginClick /deep/ .is-top .el-tabs__item  {\n    font-weight: 800;\n    text-align: center;\n    font-size: 20px;\n}\n.loginClick /deep/ .is-top .el-tabs__active-bar{\n  background-color:#fff;\n}\n.loginClick /deep/ .is-top .el-tabs__nav-wrap::after{\n  background-color: #fff;\n}\n.loginClick /deep/ .is-top{\n  margin: 0 7px;\n}\n.verification{\n  width: 60%;\n  float: left;\n}\n.codeClick{\n  width: 40%;\n  float: left;\n  text-align: center;\n  cursor: pointer;\n}\n.hint{\n  color: #999;\n  font-size:14px;\n  font-weight: 500;\n  margin-bottom: 22px;\n}\n.hint:before,.hint:after{\n  content: \"\";\n  width: 83px;\n  border-top: 1px #999 solid;\n  display: inline-block;\n  vertical-align: middle;\n}\n.hint:before{\n  margin-right: 10px;\n}\n/* .hint:after{\n  margin-left: 10px;\n} */\n.forgetDiv{\n  text-align: right;\n}\n.forgetPassword{\n  border: 0;\n  background: none;\n  padding: 0;\n}\n.pwdLogin{\n   margin-bottom:-5px;\n}\n\n.login-wrap {\n  box-sizing: border-box;\n  width: 100%;\n  height: 100%;\n  padding-top: 10%;\n  background-image: url(data:image/svg+xml;base64,);\n  /* background-color: #112346; */\n  background-repeat: no-repeat;\n  background-position: center right;\n  background-size: 100%;\n}\n.login-container {\n  border-radius: 10px;\n  margin: 0px auto;\n  width: 350px;\n  padding: 30px 35px 15px 35px;\n  background: #fff;\n  border: 1px solid #eaeaea;\n  text-align: left;\n  box-shadow: 0 0 20px 2px rgba(0, 0, 0, 0.1);\n}\n.title {\n  margin: 0px auto 40px auto;\n  text-align: center;\n  color: #505458;\n}\n.remember {\n  margin: 0px 0px 35px 0px;\n}\n.code-box {\n  text-align: right;\n}\n.codeimg {\n  height: 40px;\n}\n</style>\n"],"sourceRoot":"src/views/login"}]}