 ls:{},\r\n         usersGetList:[],\r\n         _jobOrderTime_Obj:{},\r\n         job_list_obj:{},\r\n         // 预警提醒列表\r\n         \"alarmSets\":[\r\n            {\r\n               \"startTime\":null,\r\n               \"endTime\":null,\r\n               \"warnType\":null,\r\n               \"localRemind\":null,\r\n               \"emailRemind\":null,\r\n               \"smsRemind\":null,\r\n               \"signerReceive\":null,\r\n               \"adminReceive\":null\r\n            }\r\n         ],\r\n         priority:{\r\n            \"01\":\"高\",\r\n            \"02\":\"中\",\r\n            \"03\":\"低\",\r\n         },\r\n         formInline:{\r\n            user: '',\r\n            region: ''\r\n         },\r\n         filtersData:{\r\n            pageIndex: 1, //当前页码\r\n            pageSize: 10, //每页数量\r\n            \"orderBy\":\"createTime\",\r\n            used: null, //有效状态\r\n            filter: \"\", //搜索关键字\r\n            name:\"\",\r\n            isStart:\"\",\r\n            fileteSingle:\"\",\r\n            createStartTime: \"\",\r\n            createEndTime: \"\",\r\n            businessType:\"\",//\r\n            receiveDept:\"\",// 接受部门\r\n         },\r\n         value: '',\r\n         storageValArr:[],\r\n         // 指派单选框\r\n         radio: '1',\r\n         // 指派\r\n         gridData: [],\r\n        dialogTableVisible: false,\r\n        dialogFormVisible: false,\r\n        form: {\r\n          name: '',\r\n          region: '',\r\n          date1: '',\r\n          date2: '',\r\n          delivery: false,\r\n          type: [],\r\n          resource: '',\r\n          desc: ''\r\n        },\r\n        formLabelWidth: '120px'\r\n      }\r\n   },\r\n   created() {\r\n      const that = this;\r\n      this.getHistoryWorkOrder_list();\r\n\r\n   },\r\n   methods: {\r\n      /**\r\n       *\r\n       */\r\n      fnDialogTableVisible() {\r\n         this.workOrderPoolAssign(this.ls);\r\n         this.dialogTableVisible = false;\r\n      },\r\n       /